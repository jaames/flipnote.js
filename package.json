{
  "name": "flipnote.js",
  "version": "5.0.0",
  "description": "API for handling Flipnote Studio's animation formats, capable of real-time playback using webGL",
  "module": "dist/flipnote.es.js",
  "main": "dist/flipnote.js",
  "types": "dist/flipnote.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "rollup -c --watch --environment DEV_SERVER,TARGET:webcomponent,BUILD:development",
    "dev": "rollup -c --environment BUILD:development",
    "build": "npm run dev && npm run build:min && npm run build:es && npm run build:webcomponent",
    "build:min": "rollup -c --environment TARGET:web,BUILD:production",
    "build:es": "rollup -c --environment TARGET:web,ES_MODULE,BUILD:production",
    "build:webcomponent": "rollup -c --environment TARGET:webcomponent,BUILD:production",
    "docs": "typedoc"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "flipnote",
    "flipnotestudio",
    "3D",
    "ugomemo",
    "ppm",
    "kwz",
    "animation",
    "decoder",
    "converter",
    "nintendo",
    "webGL",
    "うごメモ",
    "パラパラマンガ"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaames/flipnote.js.git"
  },
  "author": "James Daniel <npm@jamesdaniel.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaames/flipnote.js/issues"
  },
  "devDependencies": {
    "@pyoner/svelte-types": "^3.4.4-2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/node": "^14.14.2",
    "rollup": "^2.15.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svgo": "^1.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-lit-plugin": "^1.2.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-pages": "^1.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "lit-element": "^2.4.0",
    "twgl.js": "^4.15.2"
  }
}
